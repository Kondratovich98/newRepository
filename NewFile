            }
  
            {
                if ( (*(str+i) == ' ') && (*(str+i+1) == ' ')) 	// два подряд идущих пробела
                {
                    for (j=i; *(str+j)!='\0'; j++)
                    *(str+j) = *(str+j+1);

                }
                else i++;

            }

        if (*(str)==' ')
            {

                for (i=0; *(str+i) != '\0'; i++)
                    *(str+i) = *(str+i+1);
            }

        printf("%s", str);

    }
    return 0;
}
